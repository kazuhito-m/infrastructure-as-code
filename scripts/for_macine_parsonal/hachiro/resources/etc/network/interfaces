source /etc/network/interfaces.d/*

auto lo
iface lo inet loopback

allow-hotplug enp0s3   # TODO 設定が終わったら実際の物理的なものに戻す。

iface enp0s3 inet static    # TODO 設定が終わったら実際の物理的なものに戻す。
address 192.168.1.236   # TODO 設定を終わったら 11 に戻す。
network 192.168.1.0
netmask 255.255.255.0
broadcast 192.168.1.255
gateway 192.168.1.1
dns-nameservers 192.168.1.5 8.8.8.8
